/** 导航 **/
.navbar{
  background: #fff;
  color: #717171;
  border-bottom:1px solid #d8d8d8;
  height: 80rpx;
  position: fixed;
  top: 0px;
  z-index: 100;
}
.navbar .item{
  width: 180rpx;
}

.navbar .left{
  width: 70%;
  /*margin-left: 10px;*/
  text-align: left;
}

.item .checkbox{
  margin:36rpx 0 0 20rpx;
}

.cartnav .item.center{
  width:calc(100% - 170rpx - 280rpx);
}

.list{
  padding-top:80rpx;
  height:calc(100% - 184rpx)
}

.list-title {
  font-size: 26rpx;
  line-height: 1;
  padding: 5px 20rpx 5px 20rpx;
}